{
    "name": "enketo-validate",
    "version": "1.7.0",
    "description": "An XForm validator around Enketo's form engine",
    "main": "src/validator.js",
    "bin": "./validate",
    "scripts": {
        "test": "nyc --reporter=html --reporter=text mocha test/spec/*.spec.js",
        "build-docs": "rimraf docs && ./node_modules/.bin/jsdoc -c jsdoc.config.js",
        "prepublish": "rollup --config && rollup --config rollup.utils.config.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/enketo/enketo-validate.git"
    },
    "keywords": [
        "Enketo",
        "XForms",
        "ODK",
        "Validator"
    ],
    "author": "Martijn van de Rijdt",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/enketo/enketo-validate/issues"
    },
    "homepage": "https://github.com/enketo/enketo-validate#readme",
    "dependencies": {
        "commander": "^2.20.0",
        "enketo-xpath-extensions-oc": "git+https://github.com/OpenClinica/enketo-xpath-extensions-oc.git#22c6b94",
        "enketo-xslt": "^1.23.0",
        "jsdom": "13.2.x",
        "libxslt": "0.7.x"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "docdash": "^1.1.1",
        "enketo-core": "^5.5.1",
        "eslint": "^6.1.0",
        "eslint-plugin-jsdoc": "^15.8.1",
        "jsdoc": "^3.6.3",
        "mocha": "^6.2.0",
        "nyc": "^14.1.1",
        "pkg": "^4.4.0",
        "rimraf": "^3.0.0",
        "rollup": "^1.17.0",
        "rollup-plugin-commonjs": "^9.3.4",
        "rollup-plugin-node-resolve": "^4.2.4"
    }
}
